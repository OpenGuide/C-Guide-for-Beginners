#include<stdio.h>
void sort(int [],int);
int main()
{	
    int a[50];
	int i,n;
	printf("Enter the array size");
	scanf("%d",&n);
	
	for(i=0;i<n;i++)
	{
		printf("Enter %dth element",i+1);
		scanf("%d",&a[i]);
	}
	sort(a,n);
	printf("soorted array is= ");
	for(i=0;i<n;i++)
	printf("%4d",a[i]);
	return 0;
}
void sort( int a[],int n)
{
	int i,j,temp;
	for(i=0;i<n-1;i++)
	{
		for(j=i+1;j<n;j++)
		{
			if (a[i]>a[j])
			{
				temp=a[i];
				a[i]=a[j];
				a[j]=temp;
			}
		}
	}
}
