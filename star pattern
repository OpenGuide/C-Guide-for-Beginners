#include<stdio.h>
int main()
{
  int i, j, n; // Declaration of variables
  scanf("%d", &n); // Taking input from user
  for (i=0; i<n; i++) // Looping over the rows
  {
    for (j=0; j<=i; j++) // Looping for the columns
      printf("*"); // Printing Pattern
    printf("\n"); // Moving to next line
  }
  return 0;
}
